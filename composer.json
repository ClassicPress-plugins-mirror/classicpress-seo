{
    "name": "classicpress/classicpress-seo",
    "description": "SEO plugin for ClassicPress",
	"keywords": [
		"classicpress",
		"seo"
	],
    "type": "classicpress-plugin",
	"support": {
		"issues": "https://github.com/ClassicPress-research/seo-basics/issues",
		"forum": "https://forums.classicpress.net",
		"wiki": "https://github.com/ClassicPress-research/seo-basics/wiki",
		"source": "https://github.com/ClassicPress-research/seo-basics"
	},
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "ClassicPress Community",
			"homepage": "https://www.classicpress.net/contributors/"
        }
    ],
	"require": {
		"php": "^7.1",
		"cmb2/cmb2": "^2.6",
		"composer/installers": "~1.0",
		"a5hleyrich/wp-background-processing": "^1.0",
		"psr/log": "^1.1"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"classmap": [
			"includes/",
			"vendor/"
		],
		"files": ["vendor/cmb2/init.php"]
	}
}
